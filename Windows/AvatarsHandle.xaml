<Window xmlns:Controls="clr-namespace:hub_client.Windows.Controls" x:Class="hub_client.Windows.AvatarsHandle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hub_client.Windows"
        mc:Ignorable="d"
        Title="Gestion des avatars" Closed="Window_Closed" ResizeMode="NoResize" Height="581.2" Width="579.2">
    <Window.Background>
        <ImageBrush x:Name="bg_brush" Stretch="UniformToFill">
            <ImageBrush.ImageSource>
                <BitmapImage UriSource="pack://siteoforigin:,,,/Assets/Background/bg_tools.jpg" CacheOption="OnLoad" CreateOptions="IgnoreImageCache"/>
            </ImageBrush.ImageSource>
        </ImageBrush>
    </Window.Background>
    <Controls:BCA_AvatarChoice x:Name="control_avatar" Width="Auto" Margin="0,0,4,13.2"/>
</Window>
